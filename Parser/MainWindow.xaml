        <CheckBox x:Name="checkBox2" Content="2 стовбець" HorizontalAlignment="Left" Margin="557,456,0,0" VerticalAlignment="Top" Checked="checkBox2_Checked" Unchecked="checkBox2_Unchecked"/>
